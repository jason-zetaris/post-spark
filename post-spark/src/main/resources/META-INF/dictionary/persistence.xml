<?xml version="1.0" encoding="UTF-8"?>

<persistence xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
	version="2.0">
	<persistence-unit name="postSparkUnit" transaction-type="RESOURCE_LOCAL">
		<provider>org.hibernate.ejb.HibernatePersistence</provider>
		<exclude-unlisted-classes>true</exclude-unlisted-classes>
		<class>com.jj.postspark.dictionary.jpa.entity.PSDatabase</class>
		<class>com.jj.postspark.dictionary.jpa.entity.PSSchema</class>
		<class>com.jj.postspark.dictionary.jpa.entity.PSTable</class>
		<class>com.jj.postspark.dictionary.jpa.entity.PSColumn</class>
		<class>com.jj.postspark.dictionary.jpa.entity.PSPartition</class>
	</persistence-unit>
</persistence>
